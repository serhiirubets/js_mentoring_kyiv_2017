{
  "name": "students",
  "version": "0.0.1",
  "description": "Students application",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "./node_modules/.bin/eslint ./src",
    "dev": "webpack-dashboard -- webpack-dev-server --progress --profile --colors",
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "run-backend": "node backend/server.js"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "isomorphic-fetch": "^2.2.1",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "css-loader": "0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.26.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "loglevel": "^1.4.1",
    "mongod": "^2.0.0",
    "mongodb": "^2.2.33",
    "nock": "^9.0.14",
    "node-sass": "^4.3.0",
    "nyc": "^11.1.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "url-loader": "0.6.2",
    "webpack": "^3.6.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js",
      "app": "<rootDir>/src/app.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
